<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="student1" class="iuh.fit.se.mssv22714321_phanquochuy_tuan06.bai1.Student">
        <property name="id" value="001"></property>
        <property name="name" value="Nguyen Van A"></property>
    </bean>

    <!-- Inject by setter -->
    <bean id="class1" class="iuh.fit.se.mssv22714321_phanquochuy_tuan06.bai1.Clazz">
        <property name="classId" value="DHKTPM17C"></property>
        <property name="className" value="Dai Hoc KTPM17C"></property>
    </bean>
    <bean id="student2" class="iuh.fit.se.mssv22714321_phanquochuy_tuan06.bai1.Student">
        <property name="id" value="002"></property>
        <property name="name" value="Nguyen Van"></property>
        <property name="clazz" ref="class1"></property>
    </bean>
    <!-- Inject by constructor -->
    <bean id="student3" class="iuh.fit.se.mssv22714321_phanquochuy_tuan06.bai1.Student">
        <constructor-arg name="id" value="003"></constructor-arg>
        <constructor-arg name="name" value="Nguyen Van Anh"></constructor-arg>
        <constructor-arg name="clazz" ref="class1"></constructor-arg>
    </bean>

    </beans>